{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "target": "es5",
    "jsx": "react",
    "module": "es2015",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "lib": ["es2017", "dom"],
    "noEmitOnError": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
  },
  "include": [
    "./data/**/*",
    "./ui/**/*"
  ]
}
